package linkedlist;

public class IntLinkedList {
	
	private IntNode head;

	public IntLinkedList(IntNode head) {
		super();
		this.head = head;
	}

	public IntLinkedList() {
		super();
		head = null;
	}
	
	public void insertFront(int value) {
